<%= form_for (@friendlist)  do |f| %>
  
  <div class="field">
    
<%= user.email %><br/>
<%= f.hidden_field  :your_email, :value =>@user.email %>
<%= f.hidden_field  :your_name, :value =>@user.name %>
<%= f.hidden_field  :friendid, :value =>user.id %>
<%= f.hidden_field  :status, :value =>"0" %>
<%= f.hidden_field  :contact_list, :value =>user.email %>
  <%= f.submit "invite", class: "btn btn-small btn-primary" %>
  </div>
<% end %>